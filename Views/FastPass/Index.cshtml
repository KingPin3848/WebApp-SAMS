@model IEnumerable<SAMS.Models.FastPassModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StudentNameMod)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StartDateTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndDateTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Duration)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BellNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StartLocationID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Student)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Room)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sem1StudSchedule)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sem2StudSchedule)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StudentNameMod)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartDateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndDateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Duration)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BellNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartLocationID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.StudentID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Room.RoomNumberMod)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sem1StudSchedule.StudentID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sem2StudSchedule.StudentID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FastPassIDMod">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FastPassIDMod">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FastPassIDMod">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
